{
    "plugin": "react",
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": "off",
        "no-console": "off",
        "react/jsx-one-expression-per-line": "off",
        "camelcase": "off"
    },
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "jquery": true
    }
}